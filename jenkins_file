pipeline {
    agent any
    stages {
        stage('GIT Checkout') {
            steps {
                git changelog: false, poll: false, url: 'https://github.com/0lhakitura/Pytest_Test_Framework.git'
            }
        }
        stage('Test') {
  steps {
    sh 'echo "Printing version of python executable"'
    sh """
      which python
      python --version
    """
    sh 'python tests.py'
  }
}
    }
}





